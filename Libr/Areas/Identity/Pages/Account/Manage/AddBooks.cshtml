@page
@model AddBooksModel
@{
    ViewData["Title"] = "Добавление книг";
    ViewData["ActivePage"] = ManageNavPages.AddBooks;
}

<h4>@ViewData["Title"]</h4>
<form method="post">
    <div class="row">
        <div class="col-md-6">

            <ul>
                <li>
                    <label>Автор:</label>
                    <input />
                </li>
                <li>
                    <label>Название книги:</label>
                    <input />
                </li>
                <li>
                    <label>Описание:</label>
                    <input />
                </li>
                <li>
                    <label>Количество:@Model.Dest</label>
                    <input />
                </li>


            </ul>
            <div>
                <div class="custom-file ">
                    <input class="custom-file-input form-control" />
                    <label class="custom-file-label text-left ">Фото книги</label>
                </div>
            </div>
            
            <button id="btn1" onclick="OnClicks" runat="server" type="submit">Save</button>

     
        </div>
    </div>
</form>
